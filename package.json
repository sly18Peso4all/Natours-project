{
  "name": "natours",
  "version": "1.0.0",
  "description": "landing page for natours",
  "main": "index.js",
  "scripts": {
    "watch:sass": "node-sass sass/main.scss css/style.css -w",
    "devserver": "live-server",
    "start": "npm-run-all --parallel devserver watch:sass",  
      
    "compile:sass": "node-sass sass/main.scss css/style.comp.css",
    "concat:css": "concat -o css/style.concat.css css/icon-fonts.css css/style.comp.css",
    "compress:css": "node-sass css/style.concat.css css/style.css --output-style compressed",
    "build:css": "npm-run-all compile:sass concat:css compress:css"
  },
    
    
  "author": "Joseph",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^9.7.5",
    "concat": "^1.0.3",
    "node-sass": "^7.0.0",
    "npm-run-all": "^4.1.5"
  },
  "dependencies": {
    "save-dev": "0.0.1-security"
  }
}
